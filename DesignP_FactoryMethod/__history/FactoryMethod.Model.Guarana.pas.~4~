unit FactoryMethod.Model.Guarana;

interface

uses
  FactoryMethod.Model.Interfaces;

type
   TModelGuarana = class(TInterfacedObject, iRefrigerante)
      private
      public
         constructor Create ;
         destructor Destroy ; override;
         class function New: iRefrigerante;
         function GetNome : String;

end;

implementation

{ TModeloGuarana }

constructor TModeloGuarana.Create;
begin

end;

destructor TModeloGuarana.Destroy;
begin

  inherited;
end;

function TModeloGuarana.GetNome: String;
begin
  Result := 'Guarana';
end;

class function TModeloGuarana.New: iRefrigerante;
begin
  Result := Self.Create;
end;

end.
