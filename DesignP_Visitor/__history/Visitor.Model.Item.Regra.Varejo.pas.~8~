      private
        FVisit : iItem;
      public
         constructor Create ;
         destructor Destroy ; override;
         class function New: iItemRegras;
         function PrecoVenda : Currency;
         function PrecoPromocao : Currency;
         function Visit(value : iItem) : iItemRegras;

end;

implementation

{ TModelItemRegraVarejo }

