unit Command.Model.Interfaces;

interface

type
    iCommand = interface
      ['{5995562C-F91A-45C0-B3C8-4D7F657B94DD}']
      function Execute : iCommand;
    end;

    iInvoker = interface
      ['{CEC0D732-947A-42FB-A282-54D4C4D83CAE}']
      function Add(value : iCommand) : iInvoker;
      function Execute : iInvoker;
    end;

implementation

end.
