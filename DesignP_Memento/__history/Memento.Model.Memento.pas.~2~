unit Memento.Model.Memento;

interface

type
   TModelMemento = class(TInterfacedObject, iMinhaInterface)
      private
      public
         constructor Create ;
         destructor Destroy ; override;
         class function New: iMinhaInterface;

end;

implementation

{ TModelMemento }

