unit Memento.Model.Cliente;

interface

uses
  Memento.Model.Interfaces;

type
   TModelCliente = class(TInterfacedObject, iCliente)
      private
        FNome : string;
        function GetNome : string;
        procedure SetNome(const value : string);
      public
         constructor Create ;
         destructor Destroy ; override;
         class function New: iCliente;
         property Nome : string read GetNome write SetNome;
         function Memento : iMemento<iCliente>;

end;

implementation

{ TModelCliente }

constructor TModelCliente.Create;
begin

end;

destructor TModelCliente.Destroy;
begin

  inherited;
end;

class function TModelCliente.New: iCliente;
begin
  Result := Self.Create;
end;

end.
