unit Interpreter.Model.Expression.Item;

interface

type
   TMinhaClass = class(TInterfacedObject, iMinhaInterface)
      private
      public
         constructor Create ;
         destructor Destroy ; override;
         class function New: iMinhaInterface;

end;

implementation

{ TMinhaClass }

constructor TMinhaClass.Create;
begin

end;

destructor TMinhaClass.Destroy;
begin

  inherited;
end;

class function TMinhaClass.New: iMinhaInterface;
begin
  Result := Self.Create;
end;

end.
